<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" >
        <layer-list >
            <item android:drawable="@drawable/icon_rectangle_checked" android:width="15dp" android:height="15dp"/>
        </layer-list>
    </item>
    <item android:state_checked="false">
        <layer-list >
            <item android:drawable="@drawable/icon_retangle_unchecked" android:width="10dp" android:height="10dp"/>
        </layer-list>
    </item>
</selector>
